{
  "name": "@konemono/svelte5-i18n",
  "version": "0.2.1",
  "description": "Internationalization (i18n) library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TsukemonoGit/sve5-i18n.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "build": "npm run test:ci && vite build && tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.27.3",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@testing-library/svelte": "^5.2.8",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "svelte": "^5.38.0",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "i18n",
    "internationalization",
    "localization",
    "l10n",
    "translation"
  ]
}
